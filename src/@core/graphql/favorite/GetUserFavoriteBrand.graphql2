query GetUserFavoriteBrand($type: String!) {
  favoriteUser(type: $type) {
    id
    data {
      id
      name
      slug
      rating
      products {
        id
      }

      logoFile {
        id
        uuid
        modelType
        modelId
        name
        originalName
        size
        mimeType
        disk
        bucketName
        orderColumn

        presignedUrl {
          url
          expiresAt
        }
      }
    }
  }
}
