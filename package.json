    "@types/react-dom": "18.2.7",
    "chart.js": "^4.3.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "core": "link: @fullcalendar/core",
