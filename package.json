{
  "name": "@ndpco/admin",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=8.7.4",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "preinstall": "npx only-allow pnpm",
    "predev": "pnpm run codegen",
    "dev-limited": "cross-env NODE_OPTIONS='--max-http-header-size=81920' next dev",
    "dev": "cross-env next dev",
    "build": "pnpm run predev && next build",
    "start": "cross-env NODE_OPTIONS='--max-http-header-size=81920' next start",
    "lint": "next lint",
    "prepare": "husky install",
    "ngrok": "ngrok http 3000",
    "analyze": "ANALYZE=true pnpm run build",
    "prettier": "prettier --plugin-search-dir=. src/**/*.{ts,tsx,css}",
    "prettier:write": "prettier --plugin-search-dir=. src/**/*.{ts,tsx,css} --write",
    "codegen": "graphql-codegen --config codegen.ts && grep -v \"graphql-request/dist/types.dom\" src/generated.ts > generated.tmp && mv generated.tmp src/generated.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "coupling-graph": "pnpx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "@formatjs/intl-enumerator": "^1.3.2",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.2.0",
    "@mantine/hooks": "^6.0.19",
    "@persian-tools/persian-tools": "^3.5.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-direction": "^1.0.1",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-portal": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
