        "@types/react-dom": "18.2.7",
        "chart.js": "^4.4.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "cmdk": "^0.2.0",
        "core": "link: @fullcalendar/core",
        "date-fns": "^2.30.0",
        "eslint": "8.49.0",
        "eslint-config-next": "13.4.19",
        "graphql": "^16.8.0",
        "graphql-request": "^6.1.0",
        "jotai": "^2.4.2",
