    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "framer-motion": "^10.16.4",
    "graphql": "^16.8.0",
    "graphql-request": "^6.1.0",
    "jotai": "^2.3.1",
    "libphonenumber-js": "^1.10.41",
